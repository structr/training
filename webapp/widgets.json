    "source": "\u003cscript type\u003d\"module\"\u003eimport { BootstrapFormWidgetHelper } from \"/assets/custom/js/widgets/BootstrapFormWidgetHelper.js\";\n\nconst setupForm \u003d () \u003d\u003e {\n\tconst type \u003d \"[dataType]\";\n\tconst current \u003d \"${current.id}\";\n\tconst formId \u003d \"[formId]\";\n\tconst afterCreateUrl \u003d \"/${page.name}\";\n\t\n\tconst formWidgetHelper \u003d new BootstrapFormWidgetHelper(type, current?.length \u003e 0 ? current : null, formId);\n\t\n\tformWidgetHelper.addEventListener(\u0027create\u0027, (entity) \u003d\u003e {\n\t\twindow.location.href \u003d afterCreateUrl + \u0027/\u0027 + entity.id;\n\t});\n\t\n\tformWidgetHelper.initialize();\t\n}\n\t\nconst main \u003d () \u003d\u003e {\n\tdocument.removeEventListener(\u0027DOMContentLoaded\u0027, main);\n\t\n\tsetupForm();\n};\n\ndocument.addEventListener(\"DOMContentLoaded\", main);\n\u003c/script\u003e",
